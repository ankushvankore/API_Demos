public class D10PostDemo1 {
  @Test
  public void addingRecord() {
	  Map<String, Object> data = new HashMap<>();
	  data.put("name", "Vibhavari");
	  data.put("job", "Tester");
	  
	  System.out.println(data);
	  
	  JSONObject jObj = new JSONObject(data);
	  System.out.println(jObj);
	  
	  
  }
}
